@model IEnumerable<Inmobiliaria_Tanuz.Models.Inmueble>

@{
    ViewData["Title"] = "PorPropietario";
    
}


<h3 style="font-family:Tahoma" class="text-center">Sus Inmuebles: </h3>
  

    <table class="table table-striped table-hover">
        <thead class="headerTable">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.PropietarioId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Direccion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Uso)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Tipo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ambientes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Precio)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Estado)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>

                    @(item.Duenio.Nombre+" "+ item.Duenio.Apellido)

               </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Direccion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Uso)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tipo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ambientes)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Precio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Estado)
            
                </td>
                <td>
                </td>
            </tr>
            }
        </tbody>
    </table>
    <a class="btn btn-outline-warning" style="font-family:Tahoma" asp-action="Index">Volver a la lista</a>