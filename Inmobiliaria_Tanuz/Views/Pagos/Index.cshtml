@model IEnumerable<Inmobiliaria_Tanuz.Models.Pagos>

@{
    ViewData["Title"] = "Index";
}

<h1 style="font-family:Tahoma">Lista de Pagos.</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContratoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NroDePago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Importe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.contrato.Inmueble.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.contrato.Inmueble.Precio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contrato.Inmueble.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NroDePago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Importe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contrato.Inmueble.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contrato.Inmueble.Precio)
            </td>
            <!--<td>
                Html.ActionLink("Actualizar", "Edit", new { id = item.IdPago }) ||
                Html.ActionLink("Detalles", "Details", new { id = item.IdPago }) ||
                Html.ActionLink("Eliminar", "Delete", new { id = item.IdPago })
            </td>-->
        </tr>
}
    </tbody>
</table>
