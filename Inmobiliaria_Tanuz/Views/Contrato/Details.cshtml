@model Inmobiliaria_Tanuz.Models.Contrato

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del contrato</h1>

<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdContrato)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdContrato)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.InquilinoId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inquilino.Apellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.InmuebleId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inmueble.Duenio.Apellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaFin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inmueble.Direccion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inmueble.Direccion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inmueble.Precio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inmueble.Precio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inmueble.Estado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inmueble.Estado)
        </dd>
    </dl>
</div>

<div>
    @Html.ActionLink("Actualizar", "Edit", new { id = Model.IdContrato }) |
    <a asp-action="Index">Volver a la lista</a>
</div>
